{
  "name": "todo-fastify-starter",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev:tsc": "tsc --watch --preserveWatchOutput",
    "dev:node": "node --enable-source-maps --watch dist/index.js",
    "dev": "concurrently \"npm run dev:tsc\" \"wait-on dist/index.js && npm run dev:node\""
  },
