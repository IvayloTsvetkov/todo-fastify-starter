  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "fastify": "^5.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/node": "^22.13.8",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.25.0",
    "wait-on": "^8.0.2"
  },
  "type": "module"
}
